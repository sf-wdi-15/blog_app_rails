<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<% form_for @user do |f| %>
	<div>
		<% f.text_field :email, placeholder: "Email" %>
	</div>

	<div>
		<%= f.text_field :password, placeholder: "Password" %>
	</div>

	<div>
		<%= f.text_field :password_confirmation, placeholder: "Password" %>
	</div>

	<div>
		<%= f.submit %>
	</div>

<% end %>

