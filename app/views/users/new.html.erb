<div class="container">
<div class="header">
<h1> Sign Up </h1>
</div>
<div class="form">
<%= form_for @user do |f| %>
    <div>
    <%= f.text_field :email, placeholder:"email" %>
    </div>
    <br>
    <div>
      <%= f.text_field :password, placeholder:"password"%>
    </div>
    <br>
    <div>
      <%= f.text_field :password_confirmation, placeholder:"password" %>
    </div>
    <br>
    <div>
      <%= f.submit %>
    </div>
<% end %>
</div>
</div>