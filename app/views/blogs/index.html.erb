<!DOCTYPE html>
<html>
<head>
  <title></title>
</head>
<body>
<div id= "container-fluid">
  
index page 

<% @blogs.each do |blog| %>

<div class="row">
<div id= "postLink" class="col-md-1"><%= link_to blog.title, blog_path(blog.id) %></div>   
  
 
<div id= "postBlog" class="col-md-11"> 
       <!-- Title: --> <%= blog.title %> <br>
      <!--  date: --> <%=  blog.date %> <br>
       <!-- Content: --> <%= blog.content %>
       <%= button_to "Delete", blog, method: :delete %>
</div> 

<% end %>
</div>
</div>
</body>
</html>